{
  "extends": "airbnb",
  "env": {
    "browser": true
    },
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-curly-spacing": [2, "always"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0],
    "linebreak-style": ["error", "windows"],
    "no-console": [0]
  }
}
